    <step-props secure="">
      <group name="">
        <property group="" name="SCRIPT">
          <value xsi:type="xs:string">import math&#xd;
import re&#xd;
&#xd;
&#xd;
from collections import Counter as Count&#xd;
word = re.compile(r"\w+")&#xd;
&#xd;
&#xd;
def cosine_similarity(sentence1, sentence2):&#xd;
  words1 = word.findall(sentence1.lower())&#xd;
  words2 = word.findall(sentence2.lower())&#xd;
&#xd;
  # Calculate the frequency of each word in each string.&#xd;
  freq1 = Count(words1)&#xd;
  freq2 = Count(words2)&#xd;
&#xd;
  dot_product = sum(freq1[word] * freq2[word] for word in freq1)&#xd;
&#xd;
  magnitude1 = math.sqrt(sum(freq1[word]**2 for word in freq1))&#xd;
  magnitude2 = math.sqrt(sum(freq2[word]**2 for word in freq2))&#xd;
&#xd;
  cosine_similarity = dot_product / (magnitude1 * magnitude2)&#xd;
&#xd;
  return round(cosine_similarity, 2)&#xd;
  &#xd;
similartity_score = cosine_similarity(sentence1, sentence2)</value>
